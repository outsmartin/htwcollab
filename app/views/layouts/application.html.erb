<!DOCTYPE html>
<html>
  <head>
    <title>Htwcollab</title>
    <%= stylesheet_link_tag    "application" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render "layouts/header" %>

    <div id="sidebar">
      <%= render "layouts/sidebar" %>
    </div>
    <div id="content">
      <%= yield %>
    </div>

    <%= render "layouts/footer" %>
  </body>
</html>
